{
  "compilerOptions": {
    "target": "ES2015",  // Changed from "es5" to fix Set iteration errors
    "lib": [
      "dom",
      "dom.iterable",
      "ES2015",  // Changed from "es6" 
      "ES2017"   // Added for better Set/Map support
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "downlevelIteration": true,  // Added to fix Set iteration errors
    // Keep your existing settings:
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "noImplicitReturns": true,
    "noImplicitThis": true
  },
  "include": [
    "src"
  ]
}